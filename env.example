# Forum Board 环境变量配置
# 复制此文件为 .env.local 并填入实际值

# MongoDB 连接字符串
# 格式: mongodb+srv://username:password@cluster.mongodb.net/database
MONGODB_URI=mongodb+srv://your_username:your_password@your_cluster.mongodb.net/forumdb

# 应用配置
NEXT_PUBLIC_APP_NAME=Forum Board
NEXT_PUBLIC_APP_URL=http://localhost:3000

# 开发环境配置
NODE_ENV=development

# Vercel 配置 (生产环境自动设置)
# NEXT_PUBLIC_VERCEL_URL=https://your-app.vercel.app 